<div class="container">
	<div class="row">
		<% @posts.each do |post| %>	
			<div class="col-md-6"><img src="/assets/logo.png" alt="..." class="img-rounded"></div>
			<div class="col-md-6">
			<h4><%= post.title %></h4>
			<p><%= post.user.username %></p>
			<p><%= post.content %></p>
			<p>Like: <span style="color: #0000ff"><%= post.like %></span></p>
			<p><a href="/foods/show/<%= post.id %>">보기</a></p>
			</div>
		<% end %>
	</div>
</div>

